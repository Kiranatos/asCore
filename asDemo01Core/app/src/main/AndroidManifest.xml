<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AsDemo01Core"
        tools:targetApi="31">

        <!-- These mean that application will be start from this activity: -->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Demo011SecondActivity"
            android:exported="false" />
        <activity
            android:name=".Demo012ThirdActivity"
            android:exported="false" />
        <activity
            android:name=".Demo012FourthActivity"
            android:exported="false" />
        <activity
            android:name=".Demo013FourthActivity"
            android:exported="false" />
        <activity
            android:name=".Demo014FifthActivity"
            android:exported="false" />

    </application>

</manifest>